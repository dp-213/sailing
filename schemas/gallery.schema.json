{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": ["items"],
  "properties": {
    "items": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["url", "caption", "credit"],
        "properties": {
          "url": {"type": "string", "format": "uri"},
          "caption": {"type": "string"},
          "credit": {"type": "string"}
        }
      }
    }
  }
}
